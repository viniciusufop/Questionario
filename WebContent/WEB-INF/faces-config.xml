<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>usuario</from-outcome>
			<to-view-id>/publico/usuario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarQuestionario</from-outcome>
			<to-view-id>/publico/listarQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/publico/login.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/restrito/mostrarUsuario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/restrito/mostrarUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>criarQuestionario</from-outcome>
			<to-view-id>/restrito/criarQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarQuestionariosUsuario</from-outcome>
			<to-view-id>/restrito/listarQuestionariosUsuario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/restrito/listarQuestionariosUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>alterarQuestionario</from-outcome>
			<to-view-id>/restrito/alterarQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>visualizarRespostaQuestionario</from-outcome>
			<to-view-id>/restrito/visualizarRespostaQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/restrito/alterarQuestionario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>alterarPergunta</from-outcome>
			<to-view-id>/restrito/alterarPergunta.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>alterarResposta</from-outcome>
			<to-view-id>/restrito/Resposta.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/restrito/alterarPergunta.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>alterarQuestionario</from-outcome>
			<to-view-id>/restrito/alterarQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/restrito/Resposta.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>alterarQuestionario</from-outcome>
			<to-view-id>/restrito/alterarQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>	
	
	<navigation-rule>
		<from-view-id>/publico/listarQuestionario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>responderQuestionario</from-outcome>
			<to-view-id>/publico/responderQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/publico/responderQuestionario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listarQuestionario</from-outcome>
			<to-view-id>/publico/listarQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarQuestionario</from-outcome>
			<to-view-id>/publico/questionarioRespondido.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/publico/questionarioRespondido.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaQuestionario</from-outcome>
			<to-view-id>/publico/listarQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>	
	
	<navigation-rule>
		<from-view-id>/restrito/visualizarRespostaQuestionario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>visualizarRespostaUsuario</from-outcome>
			<to-view-id>/restrito/visualizarRespostaUsuario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/restrito/visualizarRespostaUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>visualizarRespostaQuestionario</from-outcome>
			<to-view-id>/restrito/visualizarRespostaQuestionario.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>		
</faces-config>
